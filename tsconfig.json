{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths":{
      "@ui/*": ["src/ui/*"],
      "@sections/*": ["src/sections/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@sanity/*": ["src/sanity/*"],
      "@constants/*": ["src/constants/*"],
    }
  }
}
